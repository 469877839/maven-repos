		<spring.version>3.2.14.RELEASE</spring.version>
		<httpclient.version>4.3.2</httpclient.version>
		<httpcore.version>4.3.2</httpcore.version>
		<log4j2.version>2.3</log4j2.version>
		<ehcache.version>2.8.6</ehcache.version>
		<dom4j.version>2.0.0-RC1</dom4j.version>
		<javassist.version>3.20.0-GA</javassist.version>
		<netty.version>4.1.0.Final</netty.version>
		<zookeeper.version>3.4.6</zookeeper.version>
		<zkclient.version>0.6</zkclient.version>
		<commons-beanutils.version>1.7.0</commons-beanutils.version>
		<commons-collections.version>3.2</commons-collections.version>
		<commons-logging.version>1.2</commons-logging.version>
		<commons-io.version>2.4</commons-io.version>
		<commons-lang.version>2.6</commons-lang.version>
		<commons-lang3.version>3.3.2</commons-lang3.version>
		<commons-net.version>3.4</commons-net.version>
		<commons-pool.version>1.6</commons-pool.version>
		<quartz.version>1.8.5</quartz.version>
		<json-lib.version>2.4</json-lib.version>
		<aspectj.version>1.7.4</aspectj.version>
		<jsch.version>0.1.52</jsch.version>
		<velocity.version>1.7</velocity.version>
		<gson.version>2.2.2</gson.version>
		<jaxrpc.version>1.1</jaxrpc.version>
		<json.version>20140107</json.version>
		<disruptor.version>3.3.2</disruptor.version>
		<junit.version>4.12</junit.version>
		<jedis.version>2.7.0</jedis.version>
		<c3p0.version>0.9.5.1</c3p0.version>
		<druid.version>1.0.31</druid.version>
	</properties>

	<dependencies>
		<dependency>
			<groupId>javax.servlet</groupId>
